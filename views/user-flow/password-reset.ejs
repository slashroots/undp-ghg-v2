<body class="body" ng-controller="RegistrationController">
  <div class="navbar w-nav" data-animation="default" data-collapse="medium" data-duration="400">
    <div class="container w-container"><a class="brand w-nav-brand" href="/"><img class="nav_countryicon" src="/images/jm_icon.png" width="27"><div class="text-block"><strong class="important-text">JAMAICA GREENHOUSE GAS INVENTORY</strong></div></a>
      <nav class="nav-menu w-nav-menu" role="navigation"><a class="w-nav-link" href="#">Home</a><a class="w-nav-link" href="#">About</a><a class="w-nav-link" href="#">Contact</a></nav>
      <div class="w-nav-button">
        <div class="w-icon-nav-menu"></div>
      </div>
    </div>
  </div>
  <div class="bodyflex">
    <div class="content w-row">
      <div class="createnew headercol w-col w-col-4">
        <div><strong>Please<br/>Enter a new<br/>Password</strong></div>
      </div>
      <div class="contentcol createnew w-col w-col-8">
        <div class="w-form">
            <p style="color: red;">{{message}}</p>
          <form id="reg-form" name="registration_form">

            <div class="createnew form-flex"><img class="field-icon" src="/images/Password-Icon.png" width="20">
              <div class="altflex">
                <label class="fieldlabel" for="Last-Name">Password</label>
                <input class="text-field w-input" id="us_password"
                    maxlength="256" name="us_password" type="password"
                    ng-model="user.us_password"
                     ng-required="true" />
                <p ng-show="registration_form.us_password.$error.required && registration_form.us_password.$dirty"
                    class="category-help-block">
                    Password is required</p>
              </div>
            </div>

            <div class="createnew form-flex"><img class="field-icon" src="/images/Password-Icon.png" width="20">
              <div class="altflex">
                <label class="fieldlabel" for="Last-Name">Confirm Password</label>
                <input class="text-field w-input" id="us_password"
                    maxlength="256" name="confirm_us_password"
                    type="password"
                    ng-model="confirm_us_password"
                    ng-required="true"/>
                <p ng-show="registration_form.confirm_us_password.$error.password_mismatch" class="category-help-block">
                                    Passwords do not match</p>
              </div>
            </div>

            <!-- <div class="button-flex"><a class="show" href="#">Show Password</a></div> -->
            <div class="button-flex createnew w-clearfix"><a class="cancel-button w-button" href="/">Cancel</a>
              <button class="submit-button w-button" ng-click="resetPassword()"
                ng-disabled="registration_form.$invalid || !tokenValid">Reset</div>
          </form>
        </div>
      </div>
    </div>
  </div>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js" type="text/javascript"></script>
