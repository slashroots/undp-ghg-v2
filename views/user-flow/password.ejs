<body class="body" ng-controller="RegistrationController">
  <div class="navbar w-nav" data-animation="default" data-collapse="medium" data-duration="400">
    <div class="container w-container"><a class="brand w-nav-brand" href="/"><img class="nav_countryicon" src="/images/jm_icon.png" width="27"><div class="text-block"><strong class="important-text">JAMAICA GREENHOUSE GAS INVENTORY</strong></div></a>
      <nav class="nav-menu w-nav-menu" role="navigation"><a class="w-nav-link" href="#">Home</a><a class="w-nav-link" href="#">About</a><a class="w-nav-link" href="#">Contact</a></nav>
      <div class="w-nav-button">
        <div class="w-icon-nav-menu"></div>
      </div>
    </div>
  </div>
  <div class="bodyflex">
    <div class="content w-row">
      <div class="createnew headercol w-col w-col-4">
        <div><strong>Request<br/>Password<br>Reset</strong></div>
      </div>
      <div class="contentcol createnew w-col w-col-8">
        <div class="w-form">
            <p style="color: red;">{{message}}</p>
          <form id="reg-form" name="registration_form" ng-if="!request_sent">

            <div class="createnew form-flex"><img class="field-icon" src="/images/Mail-Icon.png" width="20">
              <div class="altflex">
                <label class="fieldlabel" for="Last-Name">Email Address</label>
                <input class="text-field w-input" id="Email-2" maxlength="256"
                    name="us_email_address" type="email"
                    ng-model="user.us_email_address"
                    ng-required="true"/>
                <p ng-show="registration_form.us_email_address.$error.required && registration_form.us_email_address.$dirty"
                    class="category-help-block">
                    Email is required</p>
                <p ng-show="registration_form.us_email_address.$invalid && registration_form.us_email_address.$dirty"
                                    class="category-help-block">
                                    Must be a valid email address</p>
              </div>
            </div>

            <!-- <div class="button-flex"><a class="show" href="#">Show Password</a></div> -->
            <div class="button-flex createnew w-clearfix"><a class="cancel-button w-button" href="/">Cancel</a>
              <button class="submit-button w-button" ng-click="requestPassword()"
                ng-disabled="registration_form.$invalid">Send Link</div>
          </form>
          <div ng-if="request_sent">
            <h3 style="color: #909090">A link to reset your password has been sent. PLease check your email for further instructions.</h3>
            <br/>
            <br/>
            <a href="/" style="font-weight: 600;">Return to Log In</a>
          </div>
        </div>
      </div>
    </div>
  </div>
